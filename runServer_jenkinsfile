node {
	def rootDir = pwd()
	echo "${rootDir}/build.groovy"
    def build = load("${rootDir}/build.groovy")
    
    stage('Handle last server'){
        build.stopServerIfItRun()
        build.killLastContainer()
    }
    
    stage('Handle new server') {
       build.runDetachedContainer(map)
       build.launchServer()
    }
}